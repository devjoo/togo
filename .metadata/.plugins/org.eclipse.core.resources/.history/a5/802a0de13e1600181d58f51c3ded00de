package fileIO;

import java.io.BufferedReader;
import java.io.FileReader;

public class InputTest {

	//사용자 정보를 불러오기
	//불러온 정보를 화면에 출력
	private void testInput() {
		
		//파일입력 나마람ㄹ말
		fileReader fr = null;
		BufferedReader br = null;
		
		try {
			//지정된 경로의 파일을 찾아옴
			fr = new FileReader("C:\\Users\\Administrator\\leejiwoo\\@temp\\");
			//찾아온 파일을 읽는걸 보조.
			br = new BufferedReader(fr);
		} catch (Exception e) {
			// TODO: handle exception
		}
		
		

	}
}
